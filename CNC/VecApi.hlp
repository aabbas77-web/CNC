	    cdDrawbox.Hint := c_strWarningL1;
	  end;
	  x0 := x0 + w + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  	cdDrawbox.Cursor := crHelp;
	    cdDrawbox.Hint := c_strWarningL2;
	  end;
	  x0 := x0 + w + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  	cdDrawbox.Cursor := crHelp;
	    cdDrawbox.Hint := c_strWarningL3;
	  end;
	  x0 := x0 + w + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  	cdDrawbox.Cursor := crHelp;
	    cdDrawbox.Hint := c_strWarningL4;
	  end;

	  // Buttons
	  w := DM.ImageListButtons.Width;
	  h := DM.ImageListButtons.Height;
	  x0 := Round((cdPointsRoom.Generator.Width - (2*w+1*m_nDelta)) / 2);
	  y0 := y0 + DM.ImageList.Height + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  	cdDrawbox.Cursor := crHandPoint;
	    cdDrawbox.Hint := c_strCloseTheLock;
	  end;
	  x0 := x0 + w + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  	cdDrawbox.Cursor := crHandPoint;
	    cdDrawbox.Hint := c_strOpenTheLock;
	  end;
  end
  else
	if(SameText(CurrentObject.ClassName,'TMasterKeyObject')) then
  begin
		d := 2;
	  w := DM.ImageList.Width;
	  h := DM.ImageList.Height;
	  x0 := Round((cdPointsMasterKey.Generator.Width - w) / 2);
	  y0 := d + d;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  	cdDrawbox.Cursor := crHandPoint;
  	  cdDrawbox.Hint := c_strMasterKey;
	  end;

		// Buttons
		w := DM.ImageListButtonsMasterLock.Width;
		h := DM.ImageListButtonsMasterLock.Height;
		x0 := Round((cdPointsMasterKey.Generator.Width - (2*w+1*m_nDelta)) / 2);
		y0 := y0 + DM.ImageList.Height + d;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  	cdDrawbox.Cursor := crHandPoint;
  	  cdDrawbox.Hint := c_strCloseAllLocks;
	  end;
		x0 := x0 + w + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  	cdDrawbox.Cursor := crHandPoint;
  	  cdDrawbox.Hint := c_strOpenAllLocks;
	  end;
  end;
end;

procedure TFormMain.cdPointsRoomObjectSetData(CurrentObject: TcdObject);
var
	wID:Word;
label LBegin;
begin
//	if(CurrentObject.ClassName <> 'TLockObject') then Exit;
  FormInputText.Caption := c_strNewRoom;
  FormInputText.VrLabel1.Caption := c_strRoomName;
