procedure TFormMain.cdPointsMasterKeyObjectDraw(CurrentObject: TcdObject);
var
  x0,y0,w,d:Integer;
begin
	d := 2;
  ActScaleSet.Canvas.Rectangle(0,0,cdPointsMasterKey.Generator.Width,cdPointsMasterKey.Generator.Height);
  ActScaleSet.Canvas.Rectangle(0+d,0+d,cdPointsMasterKey.Generator.Width-d,cdPointsMasterKey.Generator.Height-d);
  w := DM.ImageList.Width;
  x0 := Round((cdPointsMasterKey.Generator.Width - w) / 2);
  y0 := d + d;
  if(TMasterKeyObject(CurrentObject).m_bLocked) then
  	DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,0)
  else
  	DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,1);

	// Buttons
	w := DM.ImageListButtonsMasterLock.Width;
	x0 := Round((cdPointsMasterKey.Generator.Width - (2*w+1*m_nDelta)) / 2);
	y0 := y0 + DM.ImageList.Height + d;
	DM.ImageListButtonsMasterLock.Draw(ActScaleSet.Canvas,x0,y0,0);
	x0 := x0 + w + m_nDelta;
	DM.ImageListButtonsMasterLock.Draw(ActScaleSet.Canvas,x0,y0,2);
end;

