      Shift: TShiftState);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  FormAllUsers: TFormAllUsers;

implementation
uses DataModule, InputUsers, ArDialogs, BuildingMessages;
{$R *.DFM}

procedure TFormAllUsers.VrShadowButton2Click(Sender: TObject);
begin
	ModalResult := mrAbort;
  Close();
end;

procedure TFormAllUsers.VrShadowButton4Click(Sender: TObject);
begin
  FormInputUsers.EditUserName.Text := DM.TableUsers.FieldByName('UserName').Value;
  FormInputUsers.EditPassword.Text := DM.TableUsers.FieldByName('Password').Value;
  FormInputUsers.ComboBoxLevel.ItemIndex := DM.TableUsers.FieldByName('Level').Value;
	if(FormInputUsers.ShowModal() <> mrOk) then Exit;
  DM.TableUsers.Edit();
	DM.TableUsers.FieldByName('UserName').Value := FormInputUsers.EditUserName.Text; 
	DM.TableUsers.FieldByName('Password').Value := FormInputUsers.EditPassword.Text;
	DM.TableUsers.FieldByName('Level').Value := FormInputUsers.ComboBoxLevel.ItemIndex;
  DM.TableUsers.Post();
end;

procedure TFormAllUsers.VrShadowButton3Click(Sender: TObject);
var
	strText:String;
begin
	strText := DM.TableUsers.FieldByName('UserName').Value;
	if(MessageDlg(c_strAreYouSureYouWantToDeleteThisUser+#10#13+strText,mtConfirmation,[mbYes,mbNo],0) = mrYes) then
  begin
  	DM.TableUsers.Delete();
  end;
end;

procedure TFormAllUsers.VrShadowButton1Click(Sender: TObject);
begin
  FormInputUsers.EditUserName.Text := '';
  FormInputUsers.EditPassword.Text := '';
  FormInputUsers.ComboBoxLevel.ItemIndex := 2;
	if(FormInputUsers.ShowModal() <> mrOk) then Exit;
  if(DM.TableUsers.Locate('UserName',FormInputUsers.EditUserName.Text,[])) then
  begin
  	MessageDlg(c_strUserExists,mtError,[mbOK],0);
    Exit;
  end;
  DM.TableUsers.Append();
	DM.TableUsers.FieldByName('UserName').Value := FormInputUsers.EditUserName.Text;
	DM.TableUsers.FieldByName('Password').Value := FormInputUsers.EditPassword.Text; 
	DM.TableUsers.FieldByName('Level').Value := FormInputUsers.ComboBoxLevel.ItemIndex;
  DM.TableUsers.Post();
end;

procedure TFormAllUsers.FormKeyDown(Sender: TObject; var Key: Word;
  Shift: TShiftState);
begin
	case Key of
		VK_RETURN:
    begin
      VrShadowButton1Click(nil);
    end;
		VK_ESCAPE:
    begin
      VrShadowButton2Click(nil);
    end;
  end;
end;

