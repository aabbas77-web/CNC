procedure TDM.DoTimer(AnyObject: TcdObject; var Params);
var
  x0,y0,w,d:Integer;
  Size:TSize;
  strText:String;
  CurrentObject:TcdObject;
begin
	if(FormMain.MarkMove.Designing) then	Exit;
  if(SameText(AnyObject.ClassName,'TLockObject')) then
  begin
  	CurrentObject := AnyObject;

		d := 2;
	  y0 := 2;

	  y0 := y0 + d;

		strText := 'Test';
		Size := ActScaleSet.Canvas.TextExtent(strText);
		y0 := y0 + Size.cy + FormMain.m_nDelta;

		// Status
		w := DM.ImageList.Width;
		x0 := Round((FormMain.cdPointsRoom.Generator.Width - (6*w+5*FormMain.m_nDelta)) / 2);
		y0 := y0 + Size.cy + FormMain.m_nDelta;
		x0 := x0 + w + FormMain.m_nDelta;
		x0 := x0 + w + FormMain.m_nDelta;
		if(TLockObject(CurrentObject).m_bWarningL1) then
	  begin
	  	TLockObject(CurrentObject).m_bL1 := not TLockObject(CurrentObject).m_bL1;
		  if(TLockObject(CurrentObject).m_bL1) then
		  	DM.ImageList.Draw(ActScaleSet.Canvas,CurrentObject.Pos_Log.x+x0,CurrentObject.Pos_Log.y+y0,4)
		  else
				DM.ImageList.Draw(ActScaleSet.Canvas,CurrentObject.Pos_Log.x+x0,CurrentObject.Pos_Log.y+y0,5);
	  end;
		x0 := x0 + w + FormMain.m_nDelta;
		if(TLockObject(CurrentObject).m_bWarningL2) then
	  begin
	  	TLockObject(CurrentObject).m_bL2 := not TLockObject(CurrentObject).m_bL2;
			if(TLockObject(CurrentObject).m_bL2) then
			  DM.ImageList.Draw(ActScaleSet.Canvas,CurrentObject.Pos_Log.x+x0,CurrentObject.Pos_Log.y+y0,6)
			else
			  DM.ImageList.Draw(ActScaleSet.Canvas,CurrentObject.Pos_Log.x+x0,CurrentObject.Pos_Log.y+y0,7);
	  end;
		x0 := x0 + w + FormMain.m_nDelta;
		if(TLockObject(CurrentObject).m_bWarningL3) then
	  begin
	  	TLockObject(CurrentObject).m_bL3 := not TLockObject(CurrentObject).m_bL3;
			if(TLockObject(CurrentObject).m_bL3) then
			  DM.ImageList.Draw(ActScaleSet.Canvas,CurrentObject.Pos_Log.x+x0,CurrentObject.Pos_Log.y+y0,8)
			else
			  DM.ImageList.Draw(ActScaleSet.Canvas,CurrentObject.Pos_Log.x+x0,CurrentObject.Pos_Log.y+y0,9);
	  end;
		x0 := x0 + w + FormMain.m_nDelta;
		if(TLockObject(CurrentObject).m_bWarningL4) then
	  	TLockObject(CurrentObject).m_bL4 := not TLockObject(CurrentObject).m_bL4;
		if(TLockObject(CurrentObject).m_bL4) then
		  DM.ImageList.Draw(ActScaleSet.Canvas,CurrentObject.Pos_Log.x+x0,CurrentObject.Pos_Log.y+y0,10)
		else
		  DM.ImageList.Draw(ActScaleSet.Canvas,CurrentObject.Pos_Log.x+x0,CurrentObject.Pos_Log.y+y0,11);
  end;
end;

