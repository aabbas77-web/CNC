procedure TDM.LoadSettings();
var
	IniFile:TIniFile;
begin
	m_strAppPath := IncludeTrailingBackslash(ExtractFilePath(Application.ExeName));

	IniFile := TIniFile.Create(ChangeFileExt(Application.ExeName,'.ini'));

	m_strBuildingFileName := IniFile.ReadString('Building','BuildingFileName',m_strAppPath+'Default.bld');
	ComPort.Port := IniFile.ReadString('Building','Port','COM1');
	ComPort.BaudRate := TBaudRate(IniFile.ReadInteger('Building','BaudRate',Integer(br9600)));
	ComPort.DataBits := TDataBits(IniFile.ReadInteger('Building','DataBits',Integer(dbEight)));
	ComPort.StopBits := TStopBits(IniFile.ReadInteger('Building','StopBits',Integer(sbOneStopBit)));

  IniFile.Free();
end;

procedure TDM.SaveSettings();
var
	IniFile:TIniFile;
begin
	IniFile := TIniFile.Create(ChangeFileExt(Application.ExeName,'.ini'));

	IniFile.WriteString('Building','BuildingFileName',m_strBuildingFileName);
	IniFile.WriteString('Building','Port',ComPort.Port);
	IniFile.WriteInteger('Building','BaudRate',Integer(ComPort.BaudRate));
	IniFile.WriteInteger('Building','DataBits',Integer(ComPort.DataBits));
	IniFile.WriteInteger('Building','StopBits',Integer(ComPort.StopBits));

  IniFile.Free();
end;

procedure TDM.LoadBuilding(FileName:String);
begin
  FormMain.LayerModule.Clear();
  FormMain.LayerModule.LoadFromFile(FileName);
  FormMain.ListBox.Clear();
  FormMain.LayerModule.AllLayers(BuildList,NoParam);
  FormMain.Caption := c_strBuilding + ' - [' + FileName + ']';
  SetAllLayersVisible(false);
  FormMain.LayerModule.LayerByNumber(1).Visible := true;
  FormMain.ListBox.ItemIndex := 0;
  FormMain.ListBoxClick(nil);
end;

