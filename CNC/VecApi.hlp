    	if(SameText(cdDataModule.Objects.Items[i].ClassName,'TLockObject')) then
      begin
			  if(DM.TableRooms.Locate('RoomID',IntToStr(TLockObject(cdDataModule.Objects.Items[i]).m_wID),[loCaseInsensitive])) then
			  begin
        	TLockObject(cdDataModule.Objects.Items[i]).m_bOpened := DM.TableRooms.FieldByName('Opened').AsBoolean;
        	TLockObject(cdDataModule.Objects.Items[i]).m_bLocked := DM.TableRooms.FieldByName('Locked').AsBoolean;
