procedure TDM.ASaveAreaExecute(Sender: TObject);
var
	Stream:TMemoryStream;
begin
{
	if(FormMain.cdDataModule.Empty()) then Exit;
  FormMain.cdDrawbox.SaveToFile(strTempAreaFile);
  if(not FileExists(strTempAreaFile)) then Exit;
  Stream := TMemoryStream.Create();
  Stream.LoadFromFile(strTempAreaFile);

	try
  	if(Stream.Size > 0) then
	  begin
    	if(not (TableArea.State in [dsInsert, dsEdit])) then	TableArea.Edit();
   		TableAreaAreaData.LoadFromStream(Stream);
  	  TableArea.Post();
	  end;
  finally
	  Stream.Free();
  	if(FileExists(strTempAreaFile)) then	DeleteFile(strTempAreaFile);
  end;
}  
end;

