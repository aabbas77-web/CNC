procedure TFormMain.MarkMoveObjectClickR(CurrentObject: TcdObject;
  Shift: TShiftState; X, Y: Integer);
var
	P:TPoint;
begin
	if(not Assigned(CurrentObject)) then	Exit;
	if(SameText(CurrentObject.ClassName,'TLockObject')) then
  begin
  	if(MarkMove.Designing) then
    begin
    	CurrObject := CurrentObject;
			P := cdDrawbox.ClientToScreen(Point(CurrentObject.Pos_Log.x+X,CurrentObject.Pos_Log.y+Y));
			DM.BcBarPopupMenuRoom.Popup(P.x,P.y);
    end;
  end;
end;

procedure TFormMain.ListBoxMouseDown(Sender: TObject; Button: TMouseButton;
  Shift: TShiftState; X, Y: Integer);
var
	P:TPoint;
begin
	if(Button = mbRight) then
  begin
  	if(MarkMove.Designing) then
    begin
	  	if(ListBox.ItemIndex >= 0) then
  	  begin
	  		P := ListBox.ClientToScreen(Point(X,Y));
				DM.BcBarPopupMenuArea.Popup(P.x,P.y);
	    end;
    end;
  end;
end;

