uses BuildingMessages, DataModule, InputText, InputUsers, Math;

{$R *.DFM}

{------------------------------------------------------------------------}
function IsPointInRect(x,y,x0,y0,w,h:Integer):boolean;
begin
  Result := ((x >= x0) and (x <= x0 + w) and (y >= y0) and (y <= y0 + h));
end;
{------------------------------------------------------------------------}
constructor TLockObject.Create(aTool: TcdCreator);
begin
  inherited Create(aTool);

  m_bOpened := false;
  m_bLocked := true;
  m_bWarningL1 := false;
  m_bWarningL2 := false;
  m_bWarningL3 := false;
  m_bWarningL4 := false;

  m_bL1 := false;
  m_bL2 := false;
  m_bL3 := false;
  m_bL4 := false;

  m_wID := 0;
  m_strName := c_strNewArea;
end;

destructor TLockObject.Destroy;
begin
  inherited Destroy;
end;

procedure TLockObject.Persist(Stream: TStream; const PstMode: TPstMode);
begin
  inherited Persist(Stream, PstMode);
  RegisterData(Stream, m_bOpened, SizeOf(m_bOpened), PstMode);
  RegisterData(Stream, m_bLocked, SizeOf(m_bLocked), PstMode);
  RegisterData(Stream, m_bWarningL1, SizeOf(m_bWarningL1), PstMode);
  RegisterData(Stream, m_bWarningL2, SizeOf(m_bWarningL2), PstMode);
  RegisterData(Stream, m_bWarningL3, SizeOf(m_bWarningL3), PstMode);
  RegisterData(Stream, m_bWarningL4, SizeOf(m_bWarningL4), PstMode);
  RegisterData(Stream, m_wID, SizeOf(m_wID), PstMode);
  RegisterData(Stream, m_strName, SizeOf(m_strName), PstMode);
end;
{------------------------------------------------------------------------}
constructor TMasterKeyObject.Create(aTool: TcdCreator);
begin
  inherited Create(aTool);
  m_bLocked := true;
  m_wID := 0;
  m_strName := c_strNewArea;
end;

destructor TMasterKeyObject.Destroy;
begin
  inherited Destroy;
end;

procedure TMasterKeyObject.Persist(Stream: TStream; const PstMode: TPstMode);
begin
  inherited Persist(Stream, PstMode);
  RegisterData(Stream, m_bLocked, SizeOf(m_bLocked), PstMode);
  RegisterData(Stream, m_wID, SizeOf(m_wID), PstMode);
  RegisterData(Stream, m_strName, SizeOf(m_strName), PstMode);
end;
{------------------------------------------------------------------------}

procedure TFormMain.cdPointsRoomObjectDraw(CurrentObject: TcdObject);
var
  x0,y0,w,d:Integer;
  Size:TSize;
  strText:String;
begin
	d := 2;
  y0 := 2;
  ActScaleSet.Canvas.Rectangle(0,0,cdPointsRoom.Generator.Width,cdPointsRoom.Generator.Height);
  ActScaleSet.Canvas.Rectangle(0+d,0+d,cdPointsRoom.Generator.Width-d,cdPointsRoom.Generator.Height-d);

  Size := ActScaleSet.Canvas.TextExtent(TLockObject(CurrentObject).m_strName);
  x0 := Round((cdPointsRoom.Generator.Width - Size.cx) / 2);
  y0 := y0 + d;
  ActScaleSet.Canvas.TextOut(x0,y0,TLockObject(CurrentObject).m_strName);

	strText := '0x' + IntToHex(TLockObject(CurrentObject).m_wID,2);
	Size := ActScaleSet.Canvas.TextExtent(strText);
	x0 := Round((cdPointsRoom.Generator.Width - Size.cx) / 2);
	y0 := y0 + Size.cy + m_nDelta;
	ActScaleSet.Canvas.TextOut(x0,y0,strText);

	// Status
	w := DM.ImageList.Width;
	x0 := Round((cdPointsRoom.Generator.Width - (6*w+5*m_nDelta)) / 2);
	y0 := y0 + Size.cy + m_nDelta;
	if(TLockObject(CurrentObject).m_bLocked) then
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,0)
	else
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,1);
	x0 := x0 + w + m_nDelta;
	if(TLockObject(CurrentObject).m_bOpened) then
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,2)
	else
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,3);
	x0 := x0 + w + m_nDelta;
	if(TLockObject(CurrentObject).m_bWarningL1) then
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,4)
	else
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,5);
	x0 := x0 + w + m_nDelta;
	if(TLockObject(CurrentObject).m_bWarningL2) then
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,6)
	else
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,7);
	x0 := x0 + w + m_nDelta;
	if(TLockObject(CurrentObject).m_bWarningL3) then
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,8)
	else
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,9);
	x0 := x0 + w + m_nDelta;
	if(TLockObject(CurrentObject).m_bWarningL4) then
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,10)
	else
	  DM.ImageList.Draw(ActScaleSet.Canvas,x0,y0,11);

	// Buttons
	w := DM.ImageListButtons.Width;
	x0 := Round((cdPointsRoom.Generator.Width - (2*w+1*m_nDelta)) / 2);
	y0 := y0 + DM.ImageList.Height + m_nDelta;
	DM.ImageListButtons.Draw(ActScaleSet.Canvas,x0,y0,0);
	x0 := x0 + w + m_nDelta;
	DM.ImageListButtons.Draw(ActScaleSet.Canvas,x0,y0,2);
end;

procedure TFormMain.cdPointsRoomObjectSetClass(
  var NewObjectClass: TCapClass);
begin
  NewObjectClass := TLockObject;
end;

procedure TFormMain.MarkMoveObjectDblClick(CurrentObject: TcdObject;
  Shift: TShiftState; X, Y: Integer);
var
  x0,y0,w,h,d:Integer;
  Size:TSize;
  strText:String;
  wID,wPrevID:Word;
label LBegin;
begin
	d := 2;
  y0 := 2;
	if(SameText(CurrentObject.ClassName,'TLockObject')) then
  begin
		// Title
	  Size := ActScaleSet.Canvas.TextExtent(TLockObject(CurrentObject).m_strName);
	  w := Size.cx;
	  h := Size.cy;
	  x0 := Round((cdPointsRoom.Generator.Width - w) / 2);
	  y0 := y0 + d;
		if(MarkMove.Designing) then
    begin
		  if(IsPointInRect(X,Y,x0,y0,w,h)) then
		  begin
			  FormInputText.Caption := c_strNewRoom;
			  FormInputText.VrLabel1.Caption := c_strRoomName;
				FormInputText.Edit1.Text := TLockObject(CurrentObject).m_strName;
		    FormInputText.Edit1.BiDiMode := bdRightToLeft;
			  if(FormInputText.ShowModal() = mrOk) then
		    begin
			  	TLockObject(CurrentObject).m_strName := FormInputText.Edit1.Text;
					cdDrawbox.Invalidate();
		  		DM.ASaveAreaExecute(nil);
		    end;
		  end;
    end;

    // ID
	  strText := IntToHex(TLockObject(CurrentObject).m_wID,2);
	  Size := ActScaleSet.Canvas.TextExtent('0x'+strText);
	  w := Size.cx;
	  h := Size.cy;
	  x0 := Round((cdPointsRoom.Generator.Width - Size.cx) / 2);
	  y0 := y0 + Size.cy + m_nDelta;
		if(MarkMove.Designing) then
    begin
		  if(IsPointInRect(X,Y,x0,y0,w,h)) then
		  begin
			  FormInputText.Caption := c_strRoomID;
			  FormInputText.VrLabel1.Caption := c_strRoomIDInHex;
				FormInputText.Edit1.Text := strText;
		    FormInputText.Edit1.BiDiMode := bdLeftToRight;
        wPrevID := TLockObject(CurrentObject).m_wID;
        wID := 0;
				LBegin:;
			  if(FormInputText.ShowModal() = mrOk) then
  				wID := StrToInt('$'+FormInputText.Edit1.Text)
			  else
					Abort();

				if(wID > 255) then
				begin
					MessageDlg(c_strIDOutOfRange,mtError,[mbOK],0);
				  goto LBegin;
				end;

				if(DM.TableIDs.Locate('ID',IntToStr(wID),[])) then
				begin
					MessageDlg(c_strIDExists,mtError,[mbOK],0);
				  goto LBegin;
				end;

				DM.TableIDs.Append();
				DM.TableIDs.FieldByName('ID').AsInteger := wID;
				DM.TableIDs.Post();

				if(DM.TableIDs.Locate('ID',IntToStr(wPrevID),[])) then
				begin
        	DM.TableIDs.Delete();
				end;

        TLockObject(CurrentObject).m_wID := wID;
				cdDrawbox.Invalidate();
		  	DM.ASaveAreaExecute(nil);
		  end;
    end;

    // Status
		y0 := y0 + Size.cy + m_nDelta;

	  // Buttons
	  w := DM.ImageListButtons.Width;
    h := DM.ImageListButtons.Height;
	  x0 := Round((cdPointsRoom.Generator.Width - (2*w+1*m_nDelta)) / 2);
		y0 := y0 + DM.ImageList.Height + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  end;
	  x0 := x0 + w + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  end;
  end
  else
	if(SameText(CurrentObject.ClassName,'TMasterKeyObject')) then
  begin

  end;
end;

procedure TFormMain.MarkMoveObjectMouseMove(CurrentObject: TcdObject;
  Shift: TShiftState; X, Y: Integer);
var
  x0,y0,w,h,d:Integer;
  Size:TSize;
  strText:String;
begin
  cdDrawbox.Cursor := crDefault;
  cdDrawbox.Hint := '';

	if(SameText(CurrentObject.ClassName,'TLockObject')) then
  begin
		d := 2;
    y0 := 2;
	  Size := ActScaleSet.Canvas.TextExtent(TLockObject(CurrentObject).m_strName);
	  w := Size.cx;
	  h := Size.cy;
	  x0 := Round((cdPointsRoom.Generator.Width - w) / 2);
