Function DBEmptyTable(DatabaseName,TableName:string): Boolean;
var Query : TQuery;
begin
  Query := TQuery.Create(nil);
  try
    Query.DatabaseName := DatabaseName;
    Query.SQL.Clear;
    Query.SQL.Add('DELETE FROM '+TableName);
    Query.ExecSQL;
    Result := true;
  finally
    Query.Free;
  end;
end;

Function EmptyTable(Table:TTable): Boolean;
begin
//	Table.Active := false;
//	Result := DBEmptyTable(Table.DatabaseName,Table.TableName);
//	DM.SessionBuilding.AddPassword(c_strBuildingPasswordPart1+c_strBuildingPasswordPart2);
//	Table.Active := true;

	Table.First();
  while(not Table.Eof) do	Table.Delete();
  Result := true;
end;

procedure AddEventToDatabase(EventType:TEventType;RoomID:Word);
var
	StartData,Address,Data:Byte;
begin
	DM.TableEvents.Append();
  DM.TableEvents.FieldByName('Date').Value := Date();
  DM.TableEvents.FieldByName('Time').Value := Time();
  DM.TableEvents.FieldByName('UserName').Value := DM.m_strUserName;
  DM.TableEvents.FieldByName('RoomID').Value := RoomID;
  DM.TableEvents.FieldByName('Event').Value := c_strUnknownEvent;
  case EventType of
		etOpenLock:
    begin
    	StartData := $55;
    	DM.ComPort.Write(StartData,1);
    	Address := RoomID;
    	DM.ComPort.Write(Address,1);
    	Data := $01;// Bin:01
    	DM.ComPort.Write(Data,1);

      DM.TableEvents.FieldByName('Event').Value := c_strOpenTheDoorEvent;
      FormMain.DXWaveList.Items.Find('OpenLock').Play(false);
    end;
		etCloseLock:
    begin
    	StartData := $55;
    	DM.ComPort.Write(StartData,1);
    	Address := RoomID;
    	DM.ComPort.Write(Address,1);
    	Data := $02;// Bin:10
    	DM.ComPort.Write(Data,1);

      DM.TableEvents.FieldByName('Event').Value := c_strCloseTheDoorEvent;
      FormMain.DXWaveList.Items.Find('CloseLock').Play(false);
    end;
		etWarningL1:
    begin
      DM.TableEvents.FieldByName('Event').Value := c_strWarningL1Event;
      FormMain.DXWaveList.Items.Find('WarningL1').Play(false);
    end;
		etWarningL2:
    begin
      DM.TableEvents.FieldByName('Event').Value := c_strWarningL2Event;
      FormMain.DXWaveList.Items.Find('WarningL2').Play(false);
    end;
		etWarningL3:
    begin
      DM.TableEvents.FieldByName('Event').Value := c_strWarningL3Event;
      FormMain.DXWaveList.Items.Find('WarningL3').Play(false);
    end;
		etWarningL4:
    begin
      DM.TableEvents.FieldByName('Event').Value := c_strWarningL4Event;
      FormMain.DXWaveList.Items.Find('WarningL4').Play(false);
    end;
  end;
  DM.TableEvents.Post();
end;
