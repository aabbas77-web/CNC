procedure TForm1.btnCopyClick(Sender: TObject);
var Stream: TMemoryStream;
    v: Integer;
begin
  // Copying Min, Max and Position of ScrollBar as "ScrollBar Position"
  // Clipboard format...
  Stream := TMemoryStream.Create;
  v := ScrollBar.Min;
  Stream.WriteBuffer(v, SizeOf(v));
  v := ScrollBar.Max;
  Stream.WriteBuffer(v, SizeOf(v));
  v := ScrollBar.Position;
  Stream.WriteBuffer(v, SizeOf(v));
  Clipboard.Clear;
  BinClipboard.Copy(Stream);
  Stream.Free;
end;

procedure TForm1.btnPasteClick(Sender: TObject);
var Stream: TMemoryStream;
    v: Integer;
begin
  // Pasting Min, Max and Position of ScrollBar as "ScrollBar Position"
  // Clipboard format...
  Stream := TMemoryStream.Create;
  if BinClipboard.Paste(Stream) then begin
    Stream.ReadBuffer(v, SizeOf(v));
    ScrollBar.Min := v;
    Stream.ReadBuffer(v, SizeOf(v));
    ScrollBar.Max := v;
    Stream.ReadBuffer(v, SizeOf(v));
    ScrollBar.Position := v;
    end
  else
    Application.MessageBox('Clipboard does not contains "ScrollBar Position" format!',
                           'Can not paste', MB_OK or MB_ICONEXCLAMATION);
  Stream.Free;
end;

procedure TForm1.btnExitClick(Sender: TObject);
begin
  Close;
end;

procedure TForm1.lblHomepageClick(Sender: TObject);
begin
  ShellExecute(Application.Handle, 'open', 'http://www.trichview.com/vcllist/', nil, nil, SW_NORMAL);
end;

