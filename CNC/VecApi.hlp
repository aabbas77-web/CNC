begin
  if(not SameText(AnyObject.ClassName,'TLockObject')) then	Exit;
  AddEventToDatabase(m_EventType,TLockObject(AnyObject).m_wID);
  DM.ImageListButtons.Draw(ActScaleSet.Canvas,AnyObject.Pos_Log.x+m_P0.x,AnyObject.Pos_Log.y+m_P0.y,1);

  case m_EventType of
		etOpenLock:
    begin
		  TLockObject(AnyObject).m_bWarningL1 := true;
    end;
