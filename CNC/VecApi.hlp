procedure TDM.ALogOnExecute(Sender: TObject);
label LBegin;
begin
	LBegin:;
  SessionBuilding.Active := true;

  TableUsers.Active := false;
  TableUsers.DatabaseName := m_strAppPath;
  TableUsers.Active := true;

  ANewProject.Enabled := false;

	FormInputUser := TFormInputUser.Create(Application);
	FormInputUser.Caption := c_strLogOn;
  FormInputUser.VrLabelUserName.Caption := c_strUserName;
  FormInputUser.VrLabelPassword.Caption := c_strPassword;
  FormInputUser.EditUserName.Text := '';
  FormInputUser.EditPassword.Text := '';
  if(FormInputUser.ShowModal() = mrOk) then
  begin
  	m_strUserName := FormInputUser.EditUserName.Text;
  	m_strPassword := FormInputUser.EditPassword.Text;
  	if(TableUsers.Locate('UserName',m_strUserName,[])) then
    begin
			if(m_strPassword = TableUsers.FieldByName('Password').Value) then
      begin
        m_nLevel := TableUsers.FieldByName('Level').Value;
        // nLevel : 0 -> Developer
        // nLevel : 1 -> Administrator
        // nLevel : 2 -> User
        FormMain.MarkMove.Designing := false;
        case m_nLevel of
					0:
          begin
						FormMain.MarkMove.Designing := true;
            ANewProject.Enabled := true;
          end;
        end;
      end
	    else
		  begin// Invalid password
	  		FormInputUser.Free();
				MessageDlg(c_strInvalidPassword,mtError,[mbOK],0);
        goto LBegin;
		  end;
    end
    else
	  begin// User Unknown
    	if((m_strUserName = c_strMaster) and (m_strPassword = c_strBuildingPasswordPart2+c_strBuildingPasswordPart1)) then
      begin
      	// Master
        m_nLevel := -1;
      end
      else
      begin
	  		FormInputUser.Free();
				MessageDlg(c_strUserUnknown,mtError,[mbOK],0);
        goto LBegin;
      end;
	  end;
  end
  else
  begin
	  FormInputUser.Free();
	  if(m_bStartup) then	FatalExit(0);
  end;
  FormInputUser.Free();
{
  TableArea.Active := false;
  TableArea.DatabaseName := strAppPath;
  TableArea.Active := true;
}
  TableRooms.Active := false;
  TableRooms.DatabaseName := m_strAppPath;
  TableRooms.Active := true;

  TableEvents.Active := false;
  TableEvents.DatabaseName := m_strAppPath;
  TableEvents.Active := true;

  TableIDs.Active := false;
  TableIDs.DatabaseName := m_strAppPath;
  TableIDs.Active := true;

  if((FormMain <> nil) and (FormInputUsers <> nil)) then
	  FormMain.StatusBar1.Panels.Items[0].Text := FormInputUsers.ComboBoxLevel.Items.Strings[DM.m_nLevel];
end;

