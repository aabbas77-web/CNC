        	TLockObject(cdDataModule.Objects.Items[i]).m_bWarningL3 := DM.TableRooms.FieldByName('WarningL3').AsBoolean;
        	TLockObject(cdDataModule.Objects.Items[i]).m_bWarningL4 := DM.TableRooms.FieldByName('WarningL4').AsBoolean;
        end;
      end;
    end;
  end;
