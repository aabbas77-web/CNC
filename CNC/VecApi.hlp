end;

procedure TFormInputUsers.FormCreate(Sender: TObject);
begin
	ComboBoxLevel.Clear();
  ComboBoxLevel.Items.Add(c_strSystemDeveloper);
  ComboBoxLevel.Items.Add(c_strSystemAdministrator);
  ComboBoxLevel.Items.Add(c_strSystemUser);

	ComboBoxLevel.ItemIndex := 2;
  FormMain.StatusBar1.Panels.Items[0].Text := FormInputUsers.ComboBoxLevel.Items.Strings[DM.m_nLevel];
end;

end.
