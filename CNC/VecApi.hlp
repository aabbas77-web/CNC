procedure TDM.ComPortRxChar(Sender: TObject; Count: Integer);
var
	CurrObject:TcdObject;
  bOpened,bLocked:Boolean;
begin
  case(m_nRecieveCounter) of
		0:
    begin
			ComPort.Read(m_byStartData,1);
		  m_nRecieveCounter := 1;
    end;
		1:
    begin
			ComPort.Read(m_byAddress,1);
		  m_nRecieveCounter := 2;
    end;
		2:
    begin
			ComPort.Read(m_byData,1);
		  m_nRecieveCounter := 0;

      CurrObject := DM.GetObject(m_byAddress);
      if(Assigned(CurrObject)) then
      begin
      	// Open
      	bOpened := TLockObject(CurrObject).m_bOpened;
      	TLockObject(CurrObject).m_bOpened := ((m_byData and $01) = $01);
        if(TLockObject(CurrObject).m_bOpened <> bOpened) then
			  	AddEventToDatabase(etOpenLock,TLockObject(CurrObject).m_wID);

        // Lock
      	bLocked := TLockObject(CurrObject).m_bLocked;
      	TLockObject(CurrObject).m_bLocked := ((m_byData and $02) = $02);
        if(TLockObject(CurrObject).m_bLocked <> bLocked) then
			  	AddEventToDatabase(etCloseLock,TLockObject(CurrObject).m_wID);
      end;
    end;
  end;
end;

end.

