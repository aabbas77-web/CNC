  	// Status
	  w := DM.ImageList.Width;
	  h := DM.ImageList.Height;
	  x0 := Round((cdPointsRoom.Generator.Width - (6*w+5*m_nDelta)) / 2);
	  y0 := y0 + Size.cy + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
	  	cdDrawbox.Cursor := crHelp;
	    cdDrawbox.Hint := c_strLockStatus;
	  end;
	  x0 := x0 + w + m_nDelta;
	  if(IsPointInRect(X,Y,x0,y0,w,h)) then
	  begin
